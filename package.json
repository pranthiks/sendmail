{
	"name": "sendmail",
	"displayName": "Send Mail",
	"description": "Sending EML files",
	"version": "1.0.0",
	"publisher": "pranthiks",
	"engines": {
		"vscode": "^1.46.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pranthiks/sendmail"
	},
	"bugs": {
		"url": "https://github.com/pranthiks/sendmail/issues"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"keywords": [
		"send",
		"mail",
		"email",
		"EML"
	],
	"icon": "images/sendmail-icon.png",
	"activationEvents": [
		"onCommand:sendmail.sendEmail"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sendmail.sendEmail",
				"title": "Send Mail: Send Email",
				"group": "send-mail"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
